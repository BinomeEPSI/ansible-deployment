{
    "production": {
        "domain": "{{ codimd_domain }}",
        "hsts": {
            "enable": true,
            "maxAgeSeconds": "31536000",
            "includeSubdomains": true,
            "preload": true
        },
        "csp": {
            "enable": true,
            "directives": {
            },
            "upgradeInsecureRequests": "auto",
            "addDefaults": true,
            "addDisqus": false,
            "addGoogleAnalytics": false
        },
        "db": {
            "dialect": "sqlite",
            "storage": "./db.codimd.prod.sqlite"
        }
    }
}